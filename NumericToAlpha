  <xsl:template name="Numeric_To_Alpha">
    <!--Converts a number between 1 and 26 to the corresponding Alpha character-->
    <xsl:param name="Input"/>

    <xsl:choose>
      <xsl:when test= "$Input='1'">
        <xsl:value-of select= "'A'"/>
      </xsl:when>
      <xsl:when test= "$Input='2'">
        <xsl:value-of select= "'B'"/>
      </xsl:when>
      <xsl:when test= "$Input='3'">
        <xsl:value-of select= "'C'"/>
      </xsl:when>
      <xsl:when test= "$Input='4'">
        <xsl:value-of select= "'D'"/>
      </xsl:when>
      <xsl:when test= "$Input='5'">
        <xsl:value-of select= "'E'"/>
      </xsl:when>
      <xsl:when test= "$Input='6'">
        <xsl:value-of select= "'F'"/>
      </xsl:when>
      <xsl:when test= "$Input='7'">
        <xsl:value-of select= "'G'"/>
      </xsl:when>
      <xsl:when test= "$Input='8'">
        <xsl:value-of select= "'H'"/>
      </xsl:when>
      <xsl:when test= "$Input='9'">
        <xsl:value-of select= "'I'"/>
      </xsl:when>
      <xsl:when test= "$Input='10'">
        <xsl:value-of select= "'J'"/>
      </xsl:when>
      <xsl:when test= "$Input='11'">
        <xsl:value-of select= "'K'"/>
      </xsl:when>
      <xsl:when test= "$Input='12'">
        <xsl:value-of select= "'L'"/>
      </xsl:when>
      <xsl:when test= "$Input='13'">
        <xsl:value-of select= "'M'"/>
      </xsl:when>
      <xsl:when test= "$Input='14'">
        <xsl:value-of select= "'N'"/>
      </xsl:when>
      <xsl:when test= "$Input='15'">
        <xsl:value-of select= "'O'"/>
      </xsl:when>
      <xsl:when test= "$Input='16'">
        <xsl:value-of select= "'P'"/>
      </xsl:when>
      <xsl:when test= "$Input='17'">
        <xsl:value-of select= "'Q'"/>
      </xsl:when>
      <xsl:when test= "$Input='18'">
        <xsl:value-of select= "'R'"/>
      </xsl:when>
      <xsl:when test= "$Input='19'">
        <xsl:value-of select= "'S'"/>
      </xsl:when>
      <xsl:when test= "$Input='20'">
        <xsl:value-of select= "'T'"/>
      </xsl:when>
      <xsl:when test= "$Input='21'">
        <xsl:value-of select= "'U'"/>
      </xsl:when>
      <xsl:when test= "$Input='22'">
        <xsl:value-of select= "'V'"/>
      </xsl:when>
      <xsl:when test= "$Input='23'">
        <xsl:value-of select= "'W'"/>
      </xsl:when>
      <xsl:when test= "$Input='24'">
        <xsl:value-of select= "'X'"/>
      </xsl:when>
      <xsl:when test= "$Input='25'">
        <xsl:value-of select= "'Y'"/>
      </xsl:when>
      <xsl:when test= "$Input='26'">
        <xsl:value-of select= "'Z'"/>
      </xsl:when>
      <xsl:otherwise>
        <xsl:value-of select="''"/>
      </xsl:otherwise>

    </xsl:choose>

  </xsl:template>
